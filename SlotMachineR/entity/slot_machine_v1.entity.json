        "default": "textures/entity/slot_machine_v1"
      },
      "geometry": {
        "default": "geometry.slot_machine_v1"
      },
      "scripts": {
        "scale": "0.9375",
        "initialize": [
          "variable.angle_slot_1 = 0.0;",
          "variable.angle_slot_2 = 0.0;",
          "variable.angle_slot_3 = 0.0;"
        ],
        "pre_animation": [
          "variable.angle_slot_1 = (query.skin_id-0.5) * 30;",
